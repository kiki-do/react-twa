{
	"name": "react-twa",
	"version": "0.0.0",
	"description": "The Ultimate React Toolkit for Telegram Web Apps",
	"author": {
		"name": "kiki-do",
		"url": "https://github.com/kiki-do"
	},
	"license": "MIT",
	"homepage": "https://github.com/kiki-do/react-twa",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kiki-do/react-twa.git",
		"directory": "packages/core"
	},
	"bugs": "https://github.com/kiki-do/react-twa/issues",
	"keywords": [
		"react",
		"telegram",
		"telegram-web-app",
		"twa",
		"telegram mini apps"
	],
	"sideEffects": false,
	"exports": {
		"types": "./dist/types/index.d.ts",
		"import": "./dist/esm/index.mjs",
		"require": "./dist/cjs/index.cjs"
	},
	"main": "dist/cjs/index.cjs",
	"module": "dist/esm/index.mjs",
	"types": "dist/types/index.d.ts",
	"files": [
		"dist"
	],
	"engines": {
		"node": ">=14"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"prepublishOnly": "pnpm unit-test run && pnpm build",
		"build": "shx rm -rf dist && vite build",
		"build:js": "tsc --project tsconfig.build.json && prettier --write src/bundle",
		"lint": "eslint . --fix",
		"lint-inspector": "npx @eslint/config-inspector@latest",
		"format": "prettier --write .",
		"pretty": "pnpm lint && pnpm format",
		"unit-test": "vitest",
		"lint-staged": "lint-staged"
	},
	"peerDependencies": {
		"@types/react": "^18 || ^19",
		"react": "^18 || ^19",
		"react-dom": "^18 || ^19"
	},
	"peerDependenciesMeta": {
		"@types/react": {
			"optional": true
		}
	},
	"dependencies": {
		"screenfull": "^6.0.2"
	},
	"devDependencies": {
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"@vitejs/plugin-react": "^4.6.0",
		"core-js": "^3.44.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"shx": "^0.4.0",
		"vite": "^7.0.4",
		"vite-plugin-dts": "^4.5.4"
	},
	"lint-staged": {
		"*.{js,ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		]
	}
}
